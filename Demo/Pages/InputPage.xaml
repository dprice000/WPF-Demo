<UserControl x:Class="Demo.Pages.InputPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}" Margin="16,28,167,16">
        <mui:ModernTab Layout="List">
            <mui:ModernTab.Links>
                <mui:Link DisplayName="Item 1" />
                <mui:Link DisplayName="Item 2" Source="/Pages/Charts.xaml" />
            </mui:ModernTab.Links>
        </mui:ModernTab>
        <StackPanel Margin="0,0,-117,0" HorizontalAlignment="Right" Width="117">
            <xctk:IntegerUpDown Value="1564" Margin="20,0,0.143,0" />
            <xctk:DateTimePicker AllowSpin="True"/>
            <Button Style="{StaticResource ContentRoot}" Content="Test"></Button>
        </StackPanel>
    </Grid>
</UserControl>
